<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory name="">
        <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
        <property name="hibernate.connection.password">teneqs</property>
        <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/sfm</property>
        <property name="hibernate.connection.username">teneqs</property>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
        <property name="show_sql">false</property>
		<property name="format_sql">true</property>
		<mapping class="com.sfm.model.Charges" />
		<mapping class="com.sfm.model.Fees" />
		<mapping class="com.sfm.model.Payment" />
		<mapping class="com.sfm.model.Role" />
		<mapping class="com.sfm.model.User" />
		<mapping class="com.sfm.model.UserProfile" />
			<mapping class="com.sfm.model.CompoundFees" />
    </session-factory>
</hibernate-configuration>
